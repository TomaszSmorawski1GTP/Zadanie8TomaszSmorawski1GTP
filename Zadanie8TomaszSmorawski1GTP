#include <iostream>
#include <fstream>

using namepsace std;

int main() 

{
	ifstream wjscie;
	ofstream wyjscie;
	wejscie.open("wej.txt");
	wyjscie.open("wyj.json");

	int t[10];

	if (wejscie.good())
	
	{
		while(wejscie.eof())	
		
		for(int i=0; i<10; i++)
		
		wejscie>>t[i]

	}
	
	int naj=t[0];
	
		for(int i=0; i<10; i+++)
		
		{
			if(naj<t[i])
			naj=t[i];
		}

	wyjscie<<"[\n{\n"najwieksza":""<<naj;

	wejscie.close();

	wyjscie.close();


	return 0;
}
